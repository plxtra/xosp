{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Verbose",
			"Override": {
				"Microsoft": "Warning",
				"System": "Warning"
			}
		}
	},
	"Redis": {
		"ConnectionString": "redis:6379,abortConnect=false,syncTimeout=10000"
	},
	"PostgreSQL": {
		"ConnectionString": "Server=${DbServer};User Id=${DbUser-Prodigy};Password=${DbPassword-Prodigy};Database=${DbName-Prodigy};Enlist=false;"
	},
	"PostgreSQLHistory": {
		"ConnectionString": "Server=${DbServer};User Id=${DbUser-Prodigy};Password=${DbPassword-Prodigy};Database=${DbName-Prodigy};Enlist=false;"
	},
	"Serialiser": {
		"UseCompression": false
	},
	"SNMP": {
		"Uri": "agentx://snmp:705"
	}
}
